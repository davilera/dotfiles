/* Confirm Dialog */

@import "colors.rasi"

* {
	background-color: #00000000;
	text-color: @fg;
	font: "JetBrainsMono Nerd Font Mono 10";
}

window {
	width: 200px;
	padding: 20px;
	background-color: @bg;
	border: 0px;
	border-radius: 0px;
	border-color: @ac;
	location: center;
	y-offset: -2em;
}

entry {
	expand: true;
	text-color: @ac;
}

